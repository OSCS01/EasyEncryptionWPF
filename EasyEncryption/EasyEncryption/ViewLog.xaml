<Window x:Class="EasyEncryption.ViewLog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EasyEncryption"
        mc:Ignorable="d"
        Title="ViewLog" Height="505.243" Width="770.088">
    <Grid>
        <ListView x:Name="LogView" HorizontalAlignment="Left" Height="458" VerticalAlignment="Top" Width="745" Margin="10,10,0,0" SelectionChanged="LogView_SelectionChanged">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Filename" DisplayMemberBinding="{Binding Originalfilename}"/>
                    <GridViewColumn Header="Owner" DisplayMemberBinding="{Binding Owner}"/>
                    <GridViewColumn Header="Downloaded By" DisplayMemberBinding="{Binding UserDownload}"/>
                    <GridViewColumn Header="Date" DisplayMemberBinding="{Binding Date}"/>
                    <GridViewColumn Header="Group" DisplayMemberBinding="{Binding shared}"/>
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Window>
