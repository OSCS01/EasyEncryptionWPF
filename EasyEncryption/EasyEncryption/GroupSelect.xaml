<Window x:Class="EasyEncryption.GroupSelect"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:EasyEncryption"
        mc:Ignorable="d"
        Title="Upload" Height="210.638" Width="260.745"
        Icon="/Images/logo.jpg">
    <Window.Resources>
        <ResourceDictionary Source="ResourceDic.xaml">
        </ResourceDictionary>
    </Window.Resources>
    <Grid>
        <ComboBox Foreground="#FF1D55A0" x:Name="grouplist" HorizontalAlignment="Left" Margin="10,70,0,0" VerticalAlignment="Top" Width="130" Height="26" FontSize="14"/>
        <Label Foreground="#FF1D55A0" Content="Group to share with:" HorizontalAlignment="Left" Margin="10,32,0,0" VerticalAlignment="Top" FontSize="14"/>
        <Button Style="{StaticResource styleButton}" x:Name="UploadBtn" Content="Upload" HorizontalAlignment="Left" Margin="168,146,0,0" VerticalAlignment="Top" Width="75" Click="UploadBtn_Click" FontSize="16"/>

    </Grid>
</Window>
